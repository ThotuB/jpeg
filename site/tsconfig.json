{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowImportingTsExtensions": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "layouts/*": [
        "layouts/*"
      ],
      "pages/*": [
        "pages/*"
      ],
      "public/*": [
        "public/*"
      ],
      "styles/*": [
        "styles/*"
      ],
      "utils/*": [
        "utils/*"
      ]
    }
  }
}